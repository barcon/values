# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  values_config = debug

else ifeq ($(config),release)
  values_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := values

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

values:
ifneq (,$(values_config))
	@echo "==== Building values ($(values_config)) ===="
	@${MAKE} --no-print-directory -C . -f values.make config=$(values_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f values.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   values"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"